{
  "name": "n8n-nodes-perfex-api",
  "version": "0.1.48",
  "description": "n8n community node for Perfex CRM",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "perfex",
    "crm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/baien/n8n-nodes-perfex",
  "author": {
    "name": "Your Name",
    "email": "your@email.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baien/n8n-nodes-perfex.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PerfexApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Perfex/Perfex.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-core": "^1.30.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.24",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
